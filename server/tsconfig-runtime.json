{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "resolveJsonModule": true,
    "allowJs": true,
    "transpileOnly": true
  },
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "strict": false,
      "noImplicitAny": false
    }
  }
}